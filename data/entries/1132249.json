ps://secure.gravatar.com/avatar/53e21508b6995fa42f2e034b87daa901?d=https://a248.e.akamai.net/assets.github.com%2Fimages%2Fgravatars%2Fgravatar-140.png","id":777161},"url":"https://api.github.com/gists/1132249/96ff24cc4f871061134de4188fa24cf4b53d3986","committed_at":"2011-07-28T06:44:45Z","version":"96ff24cc4f871061134de4188fa24cf4b53d3986","change_status":{"deletions":1,"additions":1,"total":2}},{"user":{"url":"https://api.github.com/users/140bytes","login":"140bytes","avatar_url":"https://secure.gravatar.com/avatar/53e21508b6995fa42f2e034b87daa901?d=https://a248.e.akamai.net/assets.github.com%2Fimages%2Fgravatars%2Fgravatar-140.png","id":777161},"url":"https://api.github.com/gists/1132249/1e1c7df97b55fe8f1543433a83d0e76a4a575055","committed_at":"2011-07-28T06:44:14Z","version":"1e1c7df97b55fe8f1543433a83d0e76a4a575055","change_status":{"deletions":2,"additions":2,"total":4}},{"user":{"url":"https://api.github.com/users/140bytes","login":"140bytes","avatar_url":"https://secure.gravatar.com/avatar/53e21508b6995fa42f2e034b87daa901?d=https://a248.e.akamai.net/assets.github.com%2Fimages%2Fgravatars%2Fgravatar-140.png","id":777161},"url":"https://api.github.com/gists/1132249/97b4f64048ad388d12e0473b416d7e4893391f50","committed_at":"2011-06-01T13:42:07Z","version":"97b4f64048ad388d12e0473b416d7e4893391f50","change_status":{"deletions":7,"additions":5,"total":12}},{"user":{"url":"https://api.github.com/users/140bytes","login":"140bytes","avatar_url":"https://secure.gravatar.com/avatar/53e21508b6995fa42f2e034b87daa901?d=https://a248.e.akamai.net/assets.github.com%2Fimages%2Fgravatars%2Fgravatar-140.png","id":777161},"url":"https://api.github.com/gists/1132249/df573b0569cc36550aa73fa9244286f24823fbb9","committed_at":"2011-06-01T12:27:17Z","version":"df573b0569cc36550aa73fa9244286f24823fbb9","change_status":{"deletions":0,"additions":1,"total":1}},{"user":{"url":"https://api.github.com/users/140bytes","login":"140bytes","avatar_url":"https://secure.gravatar.com/avatar/53e21508b6995fa42f2e034b87daa901?d=https://a248.e.akamai.net/assets.github.com%2Fimages%2Fgravatars%2Fgravatar-140.png","id":777161},"url":"https://api.github.com/gists/1132249/2da28b4a103c6a4ca00d4d0fa808ee258b5d6eca","committed_at":"2011-05-31T22:34:03Z","version":"2da28b4a103c6a4ca00d4d0fa808ee258b5d6eca","change_status":{"deletions":1,"additions":1,"total":2}},{"user":{"url":"https://api.github.com/users/140bytes","login":"140bytes","avatar_url":"https://secure.gravatar.com/avatar/53e21508b6995fa42f2e034b87daa901?d=https://a248.e.akamai.net/assets.github.com%2Fimages%2Fgravatars%2Fgravatar-140.png","id":777161},"url":"https://api.github.com/gists/1132249/cb7ce28cbcfb095a319f290d0632332f22d90c72","committed_at":"2011-05-31T18:15:34Z","version":"cb7ce28cbcfb095a319f290d0632332f22d90c72","change_status":{"deletions":0,"additions":13,"total":13}}],"comments":11,"updated_at":"2011-08-08T18:04:15Z","public":true,"forks":[],"html_url":"https://gist.github.com/1132249","git_pull_url":"git://gist.github.com/1132249.git","id":"1132249","files":{"annotated.js":{"filename":"annotated.js","content":"function(\n  a // the value to be fetch the type of\n) {\n    // first: test if the value is a primitive\n    return (function() {\n      return a && a !== this // if the value passed was a literally-created primitive, \n    }).call(a)\n    ?\n      typeof a // return its type, e.g. \"boolean\", \"number\"\n    :\n      ({}).toString.call(a).match(/\\s([a-z|A-Z]+)/)[1] // otherwise grab it's [[Class]] representation, e.g. [object Array] returns \"Array\"\n} // [object Boolean] from new Boolean(true) return \"Boolean\"","raw_url":"https://gist.github.com/raw/1132249/fa154def1fa829ac9f880bcdbb3287972c5fd9f2/annotated.js","size":503},"LICENSE.txt":{"filename":"LICENSE.txt","content":"            DO WHAT THE FUCK YOU WANT TO PUBLIC LICENSE\n                    Version 2, December 2004\n\n Copyright (C) 2011 Eli Perelman <http://eliperelman.com>\n\n Everyone is permitted to copy and distribute verbatim or modified\n copies of this license document, and changing it is allowed as long\n as the name is changed.\n\n            DO WHAT THE FUCK YOU WANT TO PUBLIC LICENSE\n   TERMS AND CONDITIONS FOR COPYING, DISTRIBUTION AND MODIFICATION\n\n  0. You just DO WHAT THE FUCK YOU WANT TO.","raw_url":"https://gist.github.com/raw/1132249/522f123a3573b451adc28cceacf848a6b64caf46/LICENSE.txt","size":490},"README.md":{"filename":"README.md","content":"Get type of of any value\n=========\n\nAdapted from these articles:\n\nhttp://javascriptweblog.wordpress.com/2011/08/08/fixing-the-javascript-typeof-operator/\n\nhttp://perfectionkills.com/instanceof-considered-harmful-or-how-to-write-a-robust-isarray/\n\nSummary:\n\nThe JavaScript `typeof` operator does not reliably return the expected types of certain values, even though it would be handy (e.g. `typeof []` should be `Array`, but it's actually `Object`). This method aims to provide a reliable way to get the real usable type of any value, including dates, regular expressions, objects, arrays, and primitive values. Read the articles for a better explanation.","raw_url":"https://gist.github.com/raw/1132249/3a7f8e5e0e728a74888c3be1e21c1d742be6426a/README.md","size":654},"index.js":{"filename":"index.js","content":"function(a){return(function(){return a&&a!==this}).call(a)?typeof a:({}).toString.call(a).match(/\\s([a-z|A-Z]+)/)[1]}","raw_url":"https://gist.github.com/raw/1132249/c3b76ab48999c43df97e28941d1fed5e88a00067/index.js","size":117},"test.html":{"filename":"test.html","content":"<!DOCTYPE html> \n<title>Get usable type of a value</title>\n<script>\n  // The problem:\n  console.log(typeof []); // logs \"object\"\n  console.log(typeof new Boolean(true)); // logs \"object\"\n  console.log(typeof /a/); // logs \"object\"\n  console.log(typeof new Date()); // logs \"object\"\n\n  // The solution:\n  var typeOf = function(a){return(function(){return a&&a!==this}).call(a)?typeof a:({}).toString.call(a).match(/\\s([a-z|A-Z]+)/)[1]};\n\n  console.log(typeOf(3)); // logs \"number\"\n  console.log(typeOf(new Number(3))); // logs \"Number\"\n  console.log(typeOf([])); // logs \"Array\"\n  console.log(typeOf(true)); // logs \"boolean\"\n  console.log(typeOf(new Boolean(true))); // logs \"Boolean\"\n  console.log(typeOf({})); // logs \"Object\"\n  console.log(typeOf(new Date())); // logs \"Date\"\n  console.log(typeOf(/[a-z]/)); // logs \"RegExp\"\n</script>","raw_url":"https://gist.github.com/raw/1132249/8083d5c8cabe68138cbc19c1d0ba7f4661c0e532/test.html","size":837},"package.json":{"filename":"package.json","content":"{\n  \"name\": \"getTrueTypeofValue\",\n\n  \"description\": \"Get the usable type of any value\",\n\n  \"keywords\": [\n    \"[[Class]]\",\n    \"typeof\",\n    \"value\",\n    \"primitive\",\n    \"object\"\n  ]\n}","raw_url":"https://gist.github.com/raw/1132249/e8e09deea99ba91b7e0634a669f487fb1d9075c2/package.json","size":184}},"git_push_url":"git@gist.github.com:1132249.git"}